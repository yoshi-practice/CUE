<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.developer.yoshi1125hisa.healthcare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cue"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cue"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BankActivity" />
        <activity android:name=".HealthActivity" />
        <activity android:name=".LogoutActivity" />
        <activity android:name=".SleepActivity"></activity>
    </application>

</manifest>